@{
    @using Website.Models;
    @model List<ShowModel>;
}
<div id="tablename">
    <h2>Status Table</h2>
</div>
<div id="statusTable">
    <table>
        <tr>
            <th>IP Address</th>
            <th>Port</th>
            <th>Job status</th>
            <th>No Of Other Client Jobs Completed</th>
        </tr>
        @foreach(var c in Model)
        {
            <tr>
                <td>@c.ip</td>
                <td>@c.Port</td>
                <td>@c.Complete</td>
                <td>@c.count</td>
            </tr>
        }
    </table>
    <script>
            setInterval(function() {
                window.location.reload();
            }, 60000);
    </script>
</div>
