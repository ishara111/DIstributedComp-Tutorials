@{
       @using WebClient.Models;
   @model List<Accinfo>;
}
<div id="tableview">
    <table>

        @foreach(var data in Model)
        {
            <tr>
                <td>@data.Id</td>
                <td>@data.fname</td>
                <td>@data.lname</td>
                <td>@data.accno</td>
                <td>@data.pin</td>
                <td>@data.balance</td>
            </tr>
        }
    </table>
</div>
