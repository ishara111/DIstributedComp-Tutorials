<Window x:Class="AsyncClient.SetUrl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncClient"
        mc:Ignorable="d"
        Title="SetUrl" Height="400" Width="300">
    <Grid>
        <TextBox x:Name="urlTextbox" HorizontalAlignment="Center" Margin="0,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Height="27"/>
        <Button x:Name="set" Content="set URL" HorizontalAlignment="Center" Margin="0,192,0,0" VerticalAlignment="Top" Width="64" Height="24" Click="set_Click"/>

    </Grid>
</Window>
